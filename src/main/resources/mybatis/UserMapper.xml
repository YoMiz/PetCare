<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" 
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.example.app.mapper.UserMapper">
<select id="showUser">
	SELECT * FROM pet_care_db.user_data
</select>
<select id="selectUserByUserId" resultType="com.example.app.domain.User">
	SELECT * FROM pet_care_db.user_data WHERE user_login = #{userLogin}
</select>
</mapper>